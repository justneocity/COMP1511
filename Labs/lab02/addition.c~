//for COMP1511 Lab 02 Exercise 2

#include <stdio.h>
int main(void) {
    int number1, number2;
    
    //Enter two numbers to add
    printf("Please enter two integers: ");
    scanf("%d %d", &number1, &number2);
 
    //Calculate and report total
    int total = number1 + number2;
    printf("%d + %d = %d\n", number1, number2, total);   
    
    return 0;

}
